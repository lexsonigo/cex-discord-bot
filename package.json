{
  "name": "cexbot",
  "version": "0.0.1",
  "description": "A Node.js app for Discord using Express and Typescript",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "npm run build",
    "postbuild": "npm run deploy-commands",
    "start": "node dist/src/server.js",
    "start:dev": "nodemon src/server.ts",
    "lint": "eslint src/** --ext .ts",
    "deploy-commands": "node dist/src/deploy-commands.js"
  },
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "express": "^4.15.2",
    "got": "^11.3.0",
    "nodemon": "^2.0.14",
    "rxjs": "^7.8.1",
    "tape": "^4.7.0",
    "ts-node": "^10.4.0",
    "typescript": "4.5"
  },
  "license": "MIT"
}
